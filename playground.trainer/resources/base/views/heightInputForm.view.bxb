input-view{
  match {
    height(height){
      to-input: getUserInfoForm
    }
  }
  message{
    template("키를 입력해주세요")
  }
  render{
    form{
      elements{
        number-input{
          id (키)
          label (키)
          type (height)
          value ("#{raw(height)}")
        }
      }
      on-submit{
        goal : userInfoStructure
        value : viv.core.FormElement(height)
      }
    }
  }
}
